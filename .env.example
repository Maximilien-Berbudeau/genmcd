# GenMCD Environment Variables
# Copy this file to .env and fill in your values

# OpenAI API Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Application Configuration
VITE_APP_NAME=GenMCD
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Automatic MCD Generator using AI

# Development Configuration
VITE_DEV_MODE=true
VITE_DEBUG_MODE=false

# Usage Limits (for freemium features)
VITE_FREE_REQUESTS_LIMIT=5
VITE_PREMIUM_REQUESTS_LIMIT=100

# Security
VITE_ENCRYPTION_KEY=your_encryption_key_here